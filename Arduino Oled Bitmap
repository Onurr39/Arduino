void drawBitmap() {
  u8g.drawBitmapP( 0, 0, 16, 64, ATATURK);
}

void setup(void) {
  
}

void loop(void) {
  u8g.firstPage();
  int i = 0;
  do {
    drawBitmap();
  } while( u8g.nextPage() );
  
  delay(1000);
}
